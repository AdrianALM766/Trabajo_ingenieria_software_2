<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="680.0" prefWidth="1100.0" styleClass="root" stylesheets="@../CSS/EstiloDashboard.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controladores.BarraLateralPrincipalController">
   <left>
      <VBox fx:id="barraLatreal" alignment="BOTTOM_CENTER" prefHeight="200.0" prefWidth="150.0" styleClass="barraLateral" stylesheets="@../CSS/EstiloDashboard.css" BorderPane.alignment="CENTER">
         <children>
            <VBox fx:id="barraLateralMenu" alignment="CENTER" prefHeight="80.0" prefWidth="100.0">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#animacionBarraLateral" prefHeight="32.0" prefWidth="32.0" stylesheets="@../CSS/EstiloDashboard.css">
                     <VBox.margin>
                        <Insets left="9.0" right="9.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <VBox fx:id="barraLateralIconos" alignment="CENTER_LEFT" prefHeight="600.0" prefWidth="150.0" spacing="10.0" stylesheets="@../CSS/EstiloDashboard.css">
               <children>
                  <Button fx:id="btnInicio" mnemonicParsing="false" onMouseClicked="#inicio" prefHeight="33.0" prefWidth="219.0" styleClass="trasparente" stylesheets="@../CSS/EstiloDashboard.css" text="Inicio">
                     <graphic>
                        <ImageView fitHeight="32.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="btnActivo">
                           <image>
                              <Image url="@../Imagenes/ImgDashboard/icon-home.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="btnProductos" mnemonicParsing="false" onMouseClicked="#productos" prefHeight="33.0" prefWidth="208.0" styleClass="trasparente" stylesheets="@../CSS/EstiloDashboard.css" text="Productos">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="btnInactivo">
                           <image>
                              <Image url="@../Imagenes/ImgDashboard/icon-producto.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="btnCategoria" mnemonicParsing="false" onMouseClicked="#categorias" prefHeight="36.0" prefWidth="174.0" styleClass="trasparente" stylesheets="@../CSS/EstiloDashboard.css" text="Categorias">
                     <graphic>
                        <ImageView fitHeight="28.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true" styleClass="btnInactivo">
                           <image>
                              <Image url="@../Imagenes/ImgDashboard/icon-categorias-de-productos.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="btnVentas" mnemonicParsing="false" onMouseClicked="#ventas" prefHeight="33.0" prefWidth="186.0" styleClass="trasparente" stylesheets="@../CSS/EstiloDashboard.css" text="Ventas">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="btnInactivo">
                           <image>
                              <Image url="@../Imagenes/ImgDashboard/icon-venta.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="btnPendientes" mnemonicParsing="false" onMouseClicked="#pendientes" prefHeight="33.0" prefWidth="249.0" styleClass="trasparente" stylesheets="@../CSS/EstiloDashboard.css" text="Pendientes">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="btnInactivo">
                           <image>
                              <Image url="@../Imagenes/ImgDashboard/icon-historial-de-pedidos.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="btnClientes" mnemonicParsing="false" onMouseClicked="#clientes" prefHeight="33.0" prefWidth="200.0" styleClass="trasparente" stylesheets="@../CSS/EstiloDashboard.css" text="Clientes">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="btnInactivo">
                           <image>
                              <Image url="@../Imagenes/ImgDashboard/icon-cliente.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="btnProveedores" mnemonicParsing="false" onMouseClicked="#proveedores" prefHeight="33.0" prefWidth="200.0" styleClass="trasparente" stylesheets="@../CSS/EstiloDashboard.css" text="Proveedores">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="btnInactivo">
                           <image>
                              <Image url="@../Imagenes/ImgDashboard/icon-proveedor.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="btnTecnicos" mnemonicParsing="false" onMouseClicked="#tecnicos" prefHeight="33.0" prefWidth="193.0" styleClass="trasparente" stylesheets="@../CSS/EstiloDashboard.css" text="Tecnicos">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="btnInactivo">
                           <image>
                              <Image url="@../Imagenes/ImgDashboard/icon-mecanico.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="btnMoto" mnemonicParsing="false" onMouseClicked="#moto" prefHeight="33.0" prefWidth="193.0" styleClass="trasparente" stylesheets="@../CSS/EstiloDashboard.css" text="Motos">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="btnInactivo">
                           <image>
                              <Image url="@../Imagenes/icon-moto-barra.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="btnDiagnostico" mnemonicParsing="false" onMouseClicked="#diagnosticos" prefHeight="33.0" prefWidth="193.0" styleClass="trasparente" stylesheets="@../CSS/EstiloDashboard.css" text="Diagnosticos">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="btnInactivo">
                           <image>
                              <Image url="@../Imagenes/icon-diagnostico.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </VBox>
         </children>
      </VBox>
   </left>
   <center>
      <VBox fx:id="contenido" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS" />
   </center>
</BorderPane>
